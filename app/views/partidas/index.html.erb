<p id="notice"><%= notice %></p>

<h1>Partidas</h1>

<table>
  <thead>
    <tr>
      <th>Titulo</th>
      <th>Description</th>
      <th>Medicion</th>
      <th>Comentario</th>
      <th>Foto</th>
      <th>Categoria</th>
      <th>Reform</th>
      <th>Habitacion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @partidas.each do |partida| %>
      <tr>
        <td><%= partida.titulo %></td>
        <td><%= partida.description %></td>
        <td><%= partida.medicion %></td>
        <td><%= partida.comentario %></td>
        <td><%= partida.foto %></td>
        <td><%= partida.categoria %></td>
        <td><%= partida.reform_id %></td>
        <td><%= partida.habitacion_id %></td>
        <td><%= link_to 'Show', partida %></td>
        <td><%= link_to 'Edit', edit_partida_path(partida) %></td>
        <td><%= link_to 'Destroy', partida, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Partida', new_partida_path %>
